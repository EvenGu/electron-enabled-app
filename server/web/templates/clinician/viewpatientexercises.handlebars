<div class="container-fluid h-100">
    <div class="row h-100">
      {{>dashboardNav}}
        <div class="col-sm-9 col-md-10">
            <div style="padding: 30px; color: #525B74;">
                <br><br>
                <h1 style= "text-align: center; font-size: 50px;" font-weight: bold; >Hello {{formatName user.name}}, Select an exercise to continue.</h1>
                <h3 align="center" style="color:#428BCA; font-size: 25px;">Your last session was on {{formatDate lastSession}}.</h3>
                <br>
                <form style= "padding-left: 50px;">
                  <!div style="padding: 0px;">
                    <label for="mySearch"></label>
                    <input type="input" id="search" name="q" onkeyup="filter()" style= "padding-left: 50px;">
                    <button id="searchButton" onclick="search()" style= "padding-bottom: 3px;">SEARCH</button>
                    <a class="btn btn-primary" id="addExercise" href="/exercise/create">ADD EXERCISE +</a>
                    <span class="validity"></span>
                 <!/div>
               </form>
               <br><br>
               <!div class="card">
                    <!div style="padding: 20px">
                       <ul id ="ul" class = "ul">
                          {{#each exercises}}
                             <li style= "padding: 10px">
                               <div class="listButtons">
                                 <a href= '/userexercise/clinician/session/{{this.patientId}}/{{this._id}}' style="color: #525B74;">{{this.exerciseName}}</a>
                               </div>
                             <li>
                          {{/each}}
                       </ul>
                    <!/div>
                <!/div>
            </div>
        </div>
    </div>
</div>
<link rel="stylesheet" href="../public/css/styleSheet.css">
<script type="text/javascript" src="../../public/scripts/clinician.js"></script>
