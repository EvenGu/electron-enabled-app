<div class="container-fluid h-100">
  <div class="row h-100">
    {{>dashboardNav}}
    <div class="col-sm-9 col-md-10">
      <div style="padding: 30px">
        <canvas id="bodyCanvas" class="bodyCanvas" style="padding-left: 0;
                                padding-right: 0;
                                left: 100px
                                margin-left: auto;
                                margin-right: auto;
                                margin-top: 30px;
                                background: grey;
                                display: block;
                                height: 800px;
                                width: 1100px;"></canvas>

        <h1 id="line1" style="position: absolute; right: 0; top: 300px">To begin the session,</h1>
        <h1 id="line2" style="position: absolute; right: 100px; top: 350px"> click start.<h1>
        <h3 id="line3" style="position: absolute; right: 100px; top: 350px color: blue; visibility: hidden;"> What would you like to do?<h3>
        <h4 style="position: absolute; right: 45px; top: 400px; color: blue;">Current Exercise: '{{exercise.exerciseName}}'<h4>
        <div class="btn btn-primary" id="startSession" onclick="start()" style="position: absolute; 
                                                       right: 50px; 
                                                       top: 450px; 
                                                       color: blue; 
                                                       width:300px; 
                                                       color:white;">START</div>
        <div class="btn btn-primary" id="discard" onclick="start()" style="position: absolute; 
                                                       right: 50px; 
                                                       top: 500px; 
                                                       color: blue;
                                                       visibility: hidden; 
                                                       width:300px; 
                                                       color:white;">DISCARD & RESTART</div>         
         <hr>
        </div>
    </div>
  </div>
</div>
<script type="text/javascript" src="/public/scripts/userexercise/beginsession.js"></script>
